{
    "name": "dwoodard/neo4j-eloquent",
    "description": "Eloquent-style API for Neo4j graph database interactions with schema-less, dynamic operations",
    "type": "library",
    "keywords": [
        "neo4j",
        "eloquent",
        "graph-database",
        "laravel",
        "cypher",
        "orm"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dustin Woodard",
            "email": "dustin@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "laudis/neo4j-php-client": "^3.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Neo4jEloquent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Neo4jEloquent\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Neo4jEloquent\\Laravel\\Neo4jEloquentServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}